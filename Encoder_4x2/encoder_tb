:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x13f804170 .scope module, "tb_Encoder_4x2" "tb_Encoder_4x2" 2 4;
 .timescale -9 -12;
v0x13f814740_0 .net "B", 1 0, v0x13f8045c0_0;  1 drivers
v0x13f814800_0 .var "D", 3 0;
v0x13f8148b0_0 .var "clk", 0 0;
E_0x13f8042f0 .event posedge, v0x13f8148b0_0;
S_0x13f804350 .scope module, "u_Encoder_4x2" "Encoder_4x2" 2 32, 3 5 0, S_0x13f804170;
 .timescale -9 -12;
    .port_info 0 /INPUT 4 "D";
    .port_info 1 /OUTPUT 2 "B";
v0x13f8045c0_0 .var "B", 1 0;
v0x13f814680_0 .net "D", 3 0, v0x13f814800_0;  1 drivers
E_0x13f804560 .event anyedge, v0x13f814680_0;
    .scope S_0x13f804350;
T_0 ;
    %wait E_0x13f804560;
    %load/vec4 v0x13f814680_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x13f8045c0_0, 0;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x13f8045c0_0, 0;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x13f8045c0_0, 0;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x13f8045c0_0, 0;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 3, 0, 2;
    %assign/vec4 v0x13f8045c0_0, 0;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x13f804170;
T_1 ;
    %delay 10000, 0;
    %load/vec4 v0x13f8148b0_0;
    %inv;
    %store/vec4 v0x13f8148b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x13f804170;
T_2 ;
    %vpi_call 2 13 "$dumpfile", "test_out.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x13f804170 {0 0 0};
    %vpi_call 2 15 "$monitor", "D=%b -> B=%b", v0x13f814800_0, v0x13f814740_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x13f804170;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13f8148b0_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x13f814800_0, 0, 4;
    %wait E_0x13f8042f0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x13f814800_0, 0, 4;
    %wait E_0x13f8042f0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x13f814800_0, 0, 4;
    %wait E_0x13f8042f0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x13f814800_0, 0, 4;
    %wait E_0x13f8042f0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x13f814800_0, 0, 4;
    %delay 20000, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_3;
